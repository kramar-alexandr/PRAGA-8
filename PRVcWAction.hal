global
function Boolean PRDClassAfterEditField(Integer wn,string fieldname,Integer fn, Integer rownr,Integer changed)
BEGIN
  Boolean res;
	
	res = inner.PRDClassAfterEditField(wn,fieldname,fn,rownr,changed);
	
	switch (fieldname) begin
		case "PRMan": res = PRDClassPRManEFAfter(wn,changed!=0);
		
		case "Suppl1": res = PRDClassSuppl1EFAfter(wn,changed!=0);
		case "Suppl2": res = PRDClassSuppl2EFAfter(wn,changed!=0);
		case "Suppl3": res = PRDClassSuppl3EFAfter(wn,changed!=0);
		case "Suppl4": res = PRDClassSuppl4EFAfter(wn,changed!=0);
		case "Suppl5": res = PRDClassSuppl5EFAfter(wn,changed!=0);
		case "Suppl6": res = PRDClassSuppl6EFAfter(wn,changed!=0);
		case "Suppl7": res = PRDClassSuppl7EFAfter(wn,changed!=0);
		case "Suppl8": res = PRDClassSuppl8EFAfter(wn,changed!=0);
		case "Suppl9": res = PRDClassSuppl9EFAfter(wn,changed!=0);
		case "Suppl10": res = PRDClassSuppl10EFAfter(wn,changed!=0);
	end;
	
  PRDClassAfterEditField = res;
  RETURN;
END;
