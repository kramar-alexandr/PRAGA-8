

function Boolean PRDClassPRManEFAfter(Integer wn,Boolean changedf)
BEGIN
  Boolean res;
  record PRVc PRr;
  record CUVc CUr;

  res = true;
  if (changedf) then begin
    GetWindowRecord(wn,PRr);
    CUr.Code = PRr.PRMan;
    if(readfirstmain(CUr,1,true))then begin
    	PRr.PRManName = CUr.Name;
    end else begin
    	PRr.PRMan = "";
    	PRr.PRManName = "";
    end;
    PutWindowRecord(wn,PRr);
  end;
  PRDClassPRManEFAfter = res;
  return;
END;

